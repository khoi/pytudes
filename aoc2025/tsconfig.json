{
	"$schema": "https://www.schemastore.org/tsconfig",
	"compilerOptions": {
		"module": "nodenext",
		"target": "es2024",
		"noEmit": true,
		"allowImportingTsExtensions": true,
		"verbatimModuleSyntax": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "nodenext",
		"types": ["bun-types"]
	}
}
